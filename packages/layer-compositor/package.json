{
  "name": "@threeify/layer-compositor",
  "description": "Typescript 3D Library loosely based on three.js",
  "version": "3.0.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "scripts": {
    "build": "threeify-build src/index.ts --bundle",
    "dev": "threeify-build src/index.ts --bundle --watch",
    "types": "tsc --emitDeclarationOnly --outDir dist --watch"
  },
  "dependencies": {
    "@threeify/core": "*",
    "@threeify/math": "*"
  },
  "devDependencies": {
    "@threeify/build": "*"
  }
}
