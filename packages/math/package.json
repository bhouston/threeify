{
  "name": "@threeify/math",
  "description": "Computer graphics oriented, High performance, tree-shakeable, TypeScript 3D vector math library.",
  "version": "4.0.3",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "types": "./dist/math.d.ts",
    "require": "./dist/math.cjs",
    "default": "./dist/math.modern.js"
  },
  "types": "./dist/math.d.ts",
  "main": "./dist/math.cjs",
  "module": "./dist/math.modern.js",
  "source": "./src/math.ts",
  "browserslist": [
    "defaults",
    "not IE 11",
    "node >= 14"
  ],
  "scripts": {
    "build": "glsl-transpiler && microbundle --format modern,cjs --define PACKAGE_VERSION=$npm_package_version",
    "watch": "glsl-transpiler --watch & microbundle watch --format modern,cjs --define PACKAGE_VERSION=$npm_package_version",
    "watch:debug": "glsl-transpiler --watch & microbundle watch --format modern,cjs --define PACKAGE_VERSION=$npm_package_version --no-compress"
  },
  "keywords": [
    "threeify",
    "computer graphics",
    "graphics",
    "3D",
    "math",
    "library"
  ],
  "homepage": "http://bhouston.github.com/threeify",
  "author": "threeify authors",
  "bugs": {
    "url": "https://github.com/bhouston/threeify/issues"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/bhouston/threeify"
  },
  "devDependencies": {
    "@types/jest": "^29.1.1",
    "jest": "^29.1.2",
    "ts-jest": "^29.0.3",
    "ts-jest-resolver": "^2.0.0",
    "typescript": "4.8.4"
  }
}
