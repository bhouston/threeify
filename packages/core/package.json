{
  "name": "@threeify/core",
  "description": "Typescript 3D Library loosely based on three.js",
  "version": "4.0.1",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "threeify",
    "webgl",
    "webgpu",
    "3D"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./dist/*": "./dist/*",
    "./shaders/*": "./dist/shaders/*"
  },
  "homepage": "http://bhouston.github.com/threeify",
  "author": "threeify authors",
  "bugs": {
    "url": "https://github.com/bhouston/threeify/issues"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/bhouston/threeify"
  },
  "devDependencies": {
    "@threeify/glsl-transpiler": "*",
    "@types/jest": "^29.1.1",
    "@types/offscreencanvas": "^2019.7.0",
    "jest": "^29.1.2",
    "ts-jest": "^29.0.3",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "4.8.4"
  },
  "scripts": {
    "build": "glsl-transpiler && tsc --build",
    "watch": "glsl-transpiler --watch & tsc --build  --watch --preserveWatchOutput",
    "test": "jest ./src"
  },
  "dependencies": {
    "@threeify/math": "*"
  }
}
