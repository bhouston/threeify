{
  "name": "threeify",
  "version": "1.87.0",
  "description": "Typescript 3D Library loosely based on three.js",
  "type": "module",
  "main": "dist/lib",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "threeify",
    "webgl",
    "webgpu",
    "3D"
  ],
  "homepage": "http://threeify.org",
  "author": "threeify authors",
  "bugs": {
    "url": "https://github.com/threeify/threeify/issues"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/threeify/threeify"
  },
  "devDependencies": {
    "@types/jest": "^29.1.1",
    "@types/offscreencanvas": "^2019.7.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "threeify-glsl-transpiler": "^2.0.6",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "build": "tgt; tsc",
    "dev": "npm run watch & npm start",
    "lint": "eslint \"src/**/*.{ts,json}\"",
    "start": "npm run watch & web-dev-server --node-resolve --watch --open --app-index ./examples/index.html",
    "docs": "typedoc",
    "test": "jest ./src",
    "watch": "tsc --watch --preserveWatchOutput & tgt --watch",
    "prettier-format": "prettier --config .prettierrc \"src/**/*.{ts,json}\" --write"
  }
}